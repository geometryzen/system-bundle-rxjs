{
    "compilerOptions": {
        "target": "es2022",
        "module": "es2015",
        "moduleResolution": "node",
        "declaration": false,
        "emitDeclarationOnly": false,
        "sourceMap": true,
        "outDir": "es2015",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitAny": true,
        "strictNullChecks": false,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": false,
        "noImplicitReturns": true,
        "skipLibCheck": true,
        "removeComments": true
    },
    "include": [
        "./src/**/*.ts"
    ]
}